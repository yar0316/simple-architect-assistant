# Streamlit Secrets Configuration Template
# Copy this file to 'secrets.toml' and update with your actual values

[aws]
# Your AWS profile name (e.g., "default", "dev", "prod")
profile = "your-aws-profile-name"

# AWS region where Bedrock is available
# Supported regions: us-east-1, us-west-2, eu-west-1, ap-southeast-1, etc.
region = "us-east-1"